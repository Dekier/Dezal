{
  "redirects": [
    {
      "source": "/:path*",
      "has": [
        {
          "type": "host",
          "value": "www.dezalroletypoznan.pl"
        }
      ],
      "destination": "https://dezalroletypoznan.pl/:path*",
      "statusCode": 308
    }
  ],
  "cleanUrls": true,
  "trailingSlash": false,
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
