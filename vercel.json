{
  "version": 2,
  "builds": [
    {
      "src": "nuxt.config.js",
      "use": "@nuxtjs/vercel-builder",
      "config": {
        "serverFiles": [
          "middleware/**",
          "plugins/**",
          "lib/**",
          "package.json",
          ".nuxt/dist/sitemap-routes.json"
        ]
      }
    }
  ],
  "redirects": [
    {
      "source": "/:path*",
      "has": [
        {
          "type": "host",
          "value": "www.dezalroletypoznan.pl"
        }
      ],
      "destination": "https://dezalroletypoznan.pl/:path*",
      "permanent": true
    }
  ]
}
